{
  "name": "baidumap",
  "version": "1.0.2",
  "description": "百度地图 for AngularJS",
  "main": "angular-baidu-map.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "browserify index.js > dist/baidu-map.js",
    "annotate": "ng-annotate -a dist/baidu-map.js > dist/baidu-map.annotated.js",
    "minify": "uglifyjs dist/baidu-map.annotated.js -c -m > dist/baidu-map.min.js",
    "build": "npm run pack && npm run annotate && npm run minify",
    "bump": "npm version patch && bower version patch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gengen1988/ng-baidu-map.git"
  },
  "author": "GG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gengen1988/ng-baidu-map/issues"
  },
  "homepage": "https://github.com/gengen1988/ng-baidu-map",
  "dependencies": {
    "angular": "^1.4.1"
  }
}
